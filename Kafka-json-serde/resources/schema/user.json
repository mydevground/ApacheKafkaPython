{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "UserProfile",
  "description": "A Confluent Kafka Python User",
  "type": "object",
  "properties": {
    "first_name": {
      "description": "User's first name",
      "type": "string"
    },
    "last_name": {
      "description": "User's last name",
      "type": "string"
    },
    "email": {
      "description": "User's email",
      "type": "string"
    },
    "age": {
      "description": "User's age",
      "type": "number",
      "exclusiveMinimum": 18
    }
  },
  "required": [
    "first_name",
    "last_name",
    "email",
    "age"
  ]
}
